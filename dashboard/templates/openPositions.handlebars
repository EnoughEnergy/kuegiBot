<div class="botContainer">
    <p><b>{{id}}</b> {{last_tick}}</p>
    <p><b>{{totalPos}}</b></p>
    <table>
        <thead>
        <tr>
            <th>PositionId</th><th>status</th><th>signalTime</th>
            <th>amount</th>
            <th>wanted</th><th>entry</th><th>entryTime</th>
            <th>initial</th><th>stop</th>
        </tr>
        </thead>
        <tbody id="positions">
        {{#each positions}}
        <tr class="{{classFromPosition this}}">
            <td>{{id}}</td>
            <td style="text-align:center">{{status}}</td>
            <td>{{formatTime signal_tstamp}}</td>
            <td>{{amount}}</td>
            <td>{{formatPrice wanted_entry}}</td>
            <td>{{formatPrice filled_entry}}</td>
            <td>{{formatTime entry_tstamp}}</td>
            <td>{{formatPrice initial_stop}}</td>
            <td></td>
        </tr>
        {{/each}}
        </tbody>
    </table>
</div>